{
  "manifest_version": 3,
  "name": "Job Application Autofill",
  "version": "1.1",
  "description": "Autofill job application forms with customizable user data",
  "permissions": ["activeTab", "scripting", "storage"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}